    "irb_prospective": {
      "description": "IRB-approved prospective collection",
      "path": "datasets/irb_handoffs/",
      "status": "pending_irb",
      "irb_protocol": null
    }
  },
  "entity_type_targets": {
    "PERSON": 100,
    "PHONE_NUMBER": 30,
    "DATE_TIME": 200,
    "LOCATION": 50,
    "MEDICAL_RECORD_NUMBER": 80,
    "ROOM": 100,
    "PEDIATRIC_AGE": 80,
    "GUARDIAN_NAME": 60
